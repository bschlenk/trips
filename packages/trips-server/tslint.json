{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],

  "rules": {
    "array-type": false,
    "interface-name": false,
    "import-name": false,
    "max-line-length": [true, { "limit": 120 }],
    "no-console": false,
    "no-increment-decrement": false,
    "object-literal-sort-keys": false
  }
}
